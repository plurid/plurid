{
    "name": "plurid-react",
    "version": "0.1.0",
    "description": "React implementation of Plurid to view and explore the web in three dimensions.",
    "author": "Jan Cavel <mail@caveljan.com> (https://caveljan.com/)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/plurid/plurid-react.git"
    },
    "bugs": {
        "url": "https://github.com/plurid/plurid-react/issues"
    },
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "jsnext:main": "dist/index.es.js",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "clean": "rm -rf ./dist",
        "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
        "test.watch": "react-scripts-ts test --env=jsdom",
        "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
        "build": "yarn clean && yarn test && yarn lint && yarn build.rollup",
        "build.rollup": "rollup -c",
        "start": "rollup -c -w"
    },
    "dependencies": {},
    "peerDependencies": {
        "prop-types": ">=15",
        "react": ">=16",
        "react-dom": ">=16",
        "styled-components": ">=4"
    },
    "devDependencies": {
        "@svgr/rollup": "^4.1.0",
        "@types/enzyme": "^3.9.0",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^24.0.9",
        "@types/react": "^16.3.13",
        "@types/react-dom": "^16.0.5",
        "@types/styled-components": "^4.1.10",
        "babel-core": "^6.26.3",
        "babel-runtime": "^6.26.0",
        "cross-env": "^5.1.4",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.10.0",
        "prettier": "^1.16.4",
        "prop-types": "^15.7.2",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-scripts-ts": "^3.1.0",
        "react-test-renderer": "^16.8.3",
        "rollup": "^1.3.0",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-commonjs": "^9.1.3",
        "rollup-plugin-node-resolve": "^4.0.1",
        "rollup-plugin-peer-deps-external": "^2.2.0",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-typescript2": "^0.19.3",
        "rollup-plugin-url": "^2.2.0",
        "styled-components": "^4.1.3",
        "tslint": "^5.13.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-react": "^3.6.0",
        "typescript": "^3.3.3333"
    }
}
