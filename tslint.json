{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": {
            "options": [
                240
            ]
        },
        "new-parens": true,
        "no-arg": true,
        "no-angle-bracket-type-assertion": false,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-console": false,
        "no-consecutive-blank-lines": false,
        "object-literal-sort-keys": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "quotemark": false
    }
}
