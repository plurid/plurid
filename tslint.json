{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": {
            "options": [
                120
            ]
        },
        "new-parens": true,
        "no-arg": true,
        "no-angle-bracket-type-assertion": false,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-console": false,
        "no-consecutive-blank-lines": false,
        "quotemark": false
    }
}
