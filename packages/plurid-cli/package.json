{
    "name": "@plurid/plurid-cli",
    "version": "0.0.9",
    "description": "plurid' application command line tool",
    "keywords": [
        "plurid",
        "application"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/plurid"
    },
    "bugs": {
        "email": "feedback@plurid.com",
        "url": "https://github.com/plurid/plurid/issues"
    },
    "homepage": "https://github.com/plurid/plurid/blob/master/packages/plurid-cli/README.md",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=6.0.0"
    },
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "typings": "distribution/types/index.d.ts",
    "bin": {
        "plurid": "binder/plurid-cli"
    },
    "files": [
        "distribution",
        "binder"
    ],
    "preferGlobal": true,
    "scripts": {
        "clean": "rm -rf ./distribution",
        "copy.server": "cp -r ./source/server ./distribution/server/",
        "test": "jest ./source --config ./jest.config.js --rootDir ./",
        "lint": "eslint ./source --ext .ts,.tsx",
        "start": "yarn clean && rollup -c rollup.config.js -w --environment ENV_MODE:local",
        "build.removeTests": "rm -rf `find ./distribution/ -type d -name __specs__`",
        "build.development": "yarn clean && rollup -c rollup.config.js --environment ENV_MODE:development",
        "build.production": "yarn clean && rollup -c --environment ENV_MODE:production",
        "build": "yarn clean && yarn lint && yarn test && yarn build.production",
        "prepublishOnly": "yarn build"
    },
    "dependencies": {
        "adm-zip": "^0.4.14",
        "apollo-cache-inmemory": "^1.6.6",
        "apollo-client": "^2.6.10",
        "apollo-link-http": "^1.5.17",
        "commander": "^5.1.0",
        "configstore": "^5.0.1",
        "cross-fetch": "^3.0.4",
        "express": "^4.17.1",
        "form-data": "^3.0.0",
        "get-port": "^5.1.1",
        "graphql": "^15.0.0",
        "graphql-tag": "^2.10.3",
        "js-yaml": "^3.14.0",
        "open": "^7.0.4",
        "parse-gitignore": "^1.0.1"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^12.0.0",
        "@rollup/plugin-json": "^4.0.3",
        "@rollup/plugin-node-resolve": "^8.0.0",
        "@rollup/plugin-replace": "^2.3.2",
        "@types/adm-zip": "^0.4.33",
        "@types/configstore": "^4.0.0",
        "@types/express": "^4.17.6",
        "@types/inquirer": "^6.5.0",
        "@types/jest": "^25.2.3",
        "@types/js-yaml": "^3.12.4",
        "@types/node": "^14.0.10",
        "@typescript-eslint/eslint-plugin": "^3.1.0",
        "@typescript-eslint/parser": "^3.1.0",
        "colors": "^1.4.0",
        "coveralls": "^3.1.0",
        "cross-env": "^7.0.2",
        "eslint": "^7.1.0",
        "jest": "^26.0.1",
        "jest-config": "^26.0.1",
        "lodash.camelcase": "^4.3.0",
        "prompt": "^1.0.0",
        "replace-in-file": "^6.0.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.13.1",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-peer-deps-external": "^2.2.2",
        "rollup-plugin-sourcemaps": "^0.6.2",
        "rollup-plugin-typescript2": "^0.27.1",
        "shelljs": "^0.8.4",
        "ts-jest": "^26.1.0",
        "ts-node": "^8.10.2",
        "typedoc": "^0.17.7",
        "typescript": "^3.9.3"
    }
}
