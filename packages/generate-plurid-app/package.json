{
    "name": "@plurid/generate-plurid-app",
    "version": "0.1.21",
    "description": "Generate a plurid' application with one command (and some choices)",
    "keywords": [
        "plurid",
        "generate",
        "application",
        "3D",
        "html",
        "react"
    ],
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/plurid"
    },
    "bugs": {
        "email": "feedback@plurid.com",
        "url": "https://github.com/plurid/plurid/issues"
    },
    "homepage": "https://github.com/plurid/plurid/blob/master/packages/generate-plurid-app/README.md",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=6.0.0"
    },
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "typings": "distribution/types/index.d.ts",
    "bin": {
        "generate-plurid-app": "binary/generate-plurid-app"
    },
    "files": [
        "distribution",
        "binary"
    ],
    "preferGlobal": true,
    "scripts": {
        "lint": "tslint --project tsconfig.json -t codeFrame 'source/**/*.ts' 'test/**/*.ts'",
        "spec": "jest --coverage",
        "spec:watch": "jest --coverage --watch",
        "spec:prod": "npm run lint && npm run spec -- --no-cache",
        "report-coverage": "cat ./coverage/lcov.info | coveralls",
        "documentation": "typedoc --out documentation --target es6 --theme minimal --mode file source",
        "start": "rollup -c rollup.config.ts -w",
        "build.removeSpecs": "rm -rf `find ./distribution/ -type d -name __specs__`",
        "build.development": "rollup -c rollup.config.ts && yarn build.removeSpecs",
        "build": "rollup -c rollup.config.ts && yarn build.removeSpecs",
        "prebuild": "rimraf distribution",
        "prepublishOnly": "yarn build"
    },
    "dependencies": {
        "@rollup/plugin-commonjs": "^11.0.2",
        "@rollup/plugin-json": "^4.0.2",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "commander": "^4.1.1",
        "inquirer": "^7.0.4"
    },
    "devDependencies": {
        "@types/inquirer": "^6.5.0",
        "@types/jest": "^25.1.3",
        "@types/node": "^13.7.4",
        "@typescript-eslint/eslint-plugin": "^2.20.0",
        "@typescript-eslint/parser": "^2.20.0",
        "colors": "^1.4.0",
        "coveralls": "^3.0.9",
        "cross-env": "^7.0.0",
        "eslint": "^6.8.0",
        "jest": "^25.1.0",
        "jest-config": "^25.1.0",
        "lodash.camelcase": "^4.3.0",
        "prompt": "^1.0.0",
        "replace-in-file": "^5.0.2",
        "rimraf": "^3.0.2",
        "rollup": "^1.31.1",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-peer-deps-external": "^2.2.2",
        "rollup-plugin-sourcemaps": "^0.5.0",
        "rollup-plugin-typescript2": "^0.26.0",
        "shelljs": "^0.8.3",
        "ts-jest": "^25.2.1",
        "ts-node": "^8.6.2",
        "typedoc": "^0.16.10",
        "typescript": "^3.8.2"
    }
}
