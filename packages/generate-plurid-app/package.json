{
    "name": "@plurid/generate-plurid-app",
    "version": "0.1.48",
    "description": "Generate a plurid' application with one command (and some choices)",
    "keywords": [
        "plurid",
        "generate",
        "application",
        "3D",
        "html",
        "react"
    ],
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/plurid"
    },
    "bugs": {
        "email": "feedback@plurid.com",
        "url": "https://github.com/plurid/plurid/issues"
    },
    "homepage": "https://github.com/plurid/plurid/blob/master/packages/generate-plurid-app/README.md",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=6.0.0"
    },
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "typings": "distribution/types/index.d.ts",
    "bin": {
        "generate-plurid-app": "binary/generate-plurid-app"
    },
    "files": [
        "distribution",
        "binary"
    ],
    "preferGlobal": true,
    "scripts": {
        "clean": "rm -rf ./distribution",
        "copy.server": "cp -r ./source/server ./distribution/server/",
        "test": "jest ./source --config ./jest.config.js --rootDir ./",
        "lint": "eslint ./source --ext .ts,.tsx",
        "start": "yarn clean && rollup -c rollup.config.js -w --environment ENV_MODE:local",
        "build.removeTests": "rm -rf `find ./distribution/ -type d -name __specs__`",
        "build.development": "yarn clean && rollup -c rollup.config.js --environment ENV_MODE:development",
        "build.production": "yarn clean && rollup -c --environment ENV_MODE:production",
        "build": "yarn clean && yarn lint && yarn test && yarn build.production",
        "prepublishOnly": "yarn build"
    },
    "dependencies": {
        "commander": "^5.0.0",
        "inquirer": "^7.1.0",
        "ora": "^4.0.4"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^11.1.0",
        "@rollup/plugin-json": "^4.0.3",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "@types/inquirer": "^6.5.0",
        "@types/jest": "^25.2.1",
        "@types/node": "^13.13.2",
        "@typescript-eslint/eslint-plugin": "^2.29.0",
        "@typescript-eslint/parser": "^2.29.0",
        "colors": "^1.4.0",
        "coveralls": "^3.0.14",
        "cross-env": "^7.0.2",
        "eslint": "^6.8.0",
        "jest": "^25.4.0",
        "jest-config": "^25.4.0",
        "lodash.camelcase": "^4.3.0",
        "prompt": "^1.0.0",
        "replace-in-file": "^5.0.2",
        "rimraf": "^3.0.2",
        "rollup": "^2.7.2",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-peer-deps-external": "^2.2.2",
        "rollup-plugin-sourcemaps": "^0.5.0",
        "rollup-plugin-typescript2": "^0.27.0",
        "shelljs": "^0.8.3",
        "ts-jest": "^25.4.0",
        "ts-node": "^8.9.0",
        "typedoc": "^0.17.4",
        "typescript": "^3.8.3"
    }
}
