{
    "name": "@plurid/generate-plurid-app",
    "version": "0.0.0-0",
    "description": "Generate a plurid' application with one command (and some choices)",
    "keywords": [
        "plurid",
        "generate",
        "application",
        "3D",
        "html",
        "react"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/plurid"
    },
    "bugs": {
        "email": "source@plurid.com",
        "url": "https://github.com/plurid/plurid/issues"
    },
    "homepage": "https://github.com/plurid/plurid/blob/master/packages/generate-plurid-app/README.md",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=12"
    },
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "typings": "distribution/types/index.d.ts",
    "bin": {
        "generate-plurid-app": "binder/generate-plurid-app"
    },
    "files": [
        "distribution",
        "binder"
    ],
    "preferGlobal": true,
    "scripts": {
        "clean": "rm -rf ./distribution",
        "copy.server": "cp -r ./source/server ./distribution/server/",
        "test": "jest ./source --config ./jest.config.js --rootDir ./",
        "lint": "eslint ./source --ext .ts,.tsx",
        "start": "yarn clean && rollup -c rollup.config.js -w --environment ENV_MODE:local",
        "build.clean": "rm -rf `find ./distribution/ -type d -name __specs__`",
        "build.development": "yarn clean && rollup -c rollup.config.js --environment ENV_MODE:development",
        "build.production": "yarn clean && rollup -c --environment ENV_MODE:production",
        "build": "yarn clean && yarn lint && yarn test && yarn build.production && yarn build.clean",
        "prepublishOnly": "yarn build"
    },
    "dependencies": {
        "commander": "^6.2.0",
        "inquirer": "^7.3.3",
        "ora": "^5.1.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^16.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^10.0.0",
        "@types/inquirer": "^7.3.1",
        "@types/jest": "^26.0.15",
        "@types/node": "^14.14.10",
        "@typescript-eslint/eslint-plugin": "^4.8.2",
        "@typescript-eslint/parser": "^4.8.2",
        "colors": "^1.4.0",
        "coveralls": "^3.1.0",
        "cross-env": "^7.0.2",
        "eslint": "^7.14.0",
        "jest": "^26.6.3",
        "jest-config": "^26.6.3",
        "lodash.camelcase": "^4.3.0",
        "prompt": "^1.0.0",
        "replace-in-file": "^6.1.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.34.0",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-typescript2": "^0.29.0",
        "shelljs": "^0.8.4",
        "ts-jest": "^26.4.4",
        "ts-node": "^9.0.0",
        "typedoc": "^0.19.2",
        "typescript": "^4.1.2"
    }
}
