{
    "name": "@plurid/generate-plurid-app",
    "version": "0.0.0-7",
    "description": "Generate a plurid' application with one command (and some choices)",
    "keywords": [
        "plurid",
        "generate",
        "application",
        "3D",
        "html",
        "react"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/plurid"
    },
    "bugs": {
        "email": "source@plurid.com",
        "url": "https://github.com/plurid/plurid/issues"
    },
    "homepage": "https://github.com/plurid/plurid/blob/master/packages/generate-plurid-app/README.md",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "typings": "distribution/types/index.d.ts",
    "bin": {
        "generate-plurid-app": "binder/generate-plurid-app"
    },
    "files": [
        "distribution",
        "binder"
    ],
    "preferGlobal": true,
    "scripts": {
        "clean": "rimraf ./distribution",
        "test": "jest ./source --config ./configurations/jest.config.js --rootDir ./",
        "lint": "eslint -c ./configurations/.eslintrc.js ./source --ext .ts,.tsx",
        "copy.server": "cp -r ./source/server ./distribution/server/",
        "start": "pnpm clean && rollup -c ./scripts/rollup.config.js  -w --environment ENV_MODE:local",
        "build.clean": "rm -rf `find ./distribution/ -type d -name __specs__`",
        "build.development": "pnpm clean && rollup -c ./scripts/rollup.config.js  --environment ENV_MODE:development",
        "build.production": "pnpm clean && pnpm lint && pnpm test && rollup -c ./scripts/rollup.config.js --environment ENV_MODE:production && pnpm build.clean",
        "build": "pnpm build.production",
        "prepublishOnly": "pnpm build"
    },
    "dependencies": {
        "commander": "^9.4.0",
        "inquirer": "^9.1.0",
        "ora": "==5.1.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^22.0.2",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@types/inquirer": "^9.0.1",
        "@types/jest": "^28.1.7",
        "@types/node": "^18.7.6",
        "@typescript-eslint/eslint-plugin": "^5.33.1",
        "@typescript-eslint/parser": "^5.33.1",
        "colors": "^1.4.0",
        "coveralls": "^3.1.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.22.0",
        "jest": "^28.1.3",
        "jest-config": "^28.1.3",
        "lodash.camelcase": "^4.3.0",
        "prompt": "^1.3.0",
        "replace-in-file": "^6.3.5",
        "rimraf": "^3.0.2",
        "rollup": "^2.78.0",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-typescript2": "^0.32.1",
        "shelljs": "^0.8.5",
        "ts-jest": "^28.0.8",
        "ts-node": "^10.9.1",
        "tslib": "^2.4.0",
        "typedoc": "^0.23.10",
        "typescript": "^4.7.4"
    }
}
