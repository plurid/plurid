{
    "name": "@plurid/plurid-react-server",
    "version": "0.0.0-0",
    "description": "React implementation of Plurid to view and explore the web in three dimensions with server-side rendering",
    "keywords": [
        "plurid",
        "react",
        "web",
        "3D",
        "html",
        "server-side rendering"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/plurid"
    },
    "bugs": {
        "email": "source@plurid.com",
        "url": "https://github.com/plurid/plurid/issues"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "homepage": "https://github.com/plurid/plurid/blob/master/packages/plurid-web/plurid-react-server",
    "files": [
        "distribution/"
    ],
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "types": "distribution/index.d.ts",
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "scripts": {
        "clean": "rm -rf ./distribution",
        "test": "jest ./source --config ./configurations/jest.config.js --rootDir ./",
        "lint": "eslint -c ./configurations/.eslintrc.js ./source --ext .ts,.tsx",
        "start": "yarn clean && rollup -c ./scripts/rollup.development.js -w --environment ENV_MODE:local",
        "build.development": "yarn clean && rollup -c ./scripts/rollup.development.js --environment ENV_MODE:development",
        "build.production": "yarn clean && rollup -c ./scripts/rollup.config.js --environment ENV_MODE:production",
        "build": "yarn clean && yarn lint && yarn test && yarn build.production",
        "esbuild": "esbuild source/index.ts --bundle --platform=node --outfile=distribution/index.js --external:react",
        "prepublishOnly": "yarn build"
    },
    "peerDependencies": {
        "@plurid/plurid-data": "*",
        "@plurid/plurid-engine": "*",
        "@plurid/plurid-functions": "*",
        "@plurid/plurid-pubsub": "*",
        "@plurid/plurid-react": "*",
        "puppeteer": ">=2.1",
        "react": ">=16.8",
        "react-dom": ">=16.8",
        "react-helmet-async": ">=1",
        "react-redux": ">=7.2",
        "redux": ">=4",
        "styled-components": ">=5"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "detect-port": "^1.3.0",
        "express": "^4.17.1",
        "open": "^8.0.6"
    },
    "devDependencies": {
        "@apollo/client": "^3.3.15",
        "@babel/core": "^7.13.16",
        "@plurid/plurid-data": "0.0.0-2",
        "@plurid/plurid-engine": "0.0.0-1",
        "@plurid/plurid-functions": "0.0.0-3",
        "@plurid/plurid-functions-react": "0.0.0-1",
        "@plurid/plurid-icons-react": "0.0.0-0",
        "@plurid/plurid-pubsub": "0.0.0-3",
        "@plurid/plurid-react": "0.0.0-2",
        "@plurid/plurid-themes": "0.0.0-0",
        "@plurid/plurid-ui-components-react": "0.0.0-1",
        "@plurid/plurid-ui-state-react": "0.0.0-0",
        "@rollup/plugin-node-resolve": "^11.2.1",
        "@types/body-parser": "^1.19.0",
        "@types/compression": "^1.7.0",
        "@types/detect-port": "^1.3.0",
        "@types/express": "^4.17.11",
        "@types/hammerjs": "^2.0.39",
        "@types/jest": "^26.0.22",
        "@types/puppeteer": "^5.4.3",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "@types/react-redux": "^7.1.16",
        "@types/react-stripe-elements": "^6.0.4",
        "@types/styled-components": "^5.1.9",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "eslint": "^7.24.0",
