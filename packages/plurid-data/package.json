        "build.production": "rollup -c --environment ENV_MODE:production",
        "build": "yarn clean && yarn lint && yarn build.production",
        "prepublishOnly": "yarn build"
    },
    "peerDependencies": {
        "@plurid/plurid-pubsub": "*"
    },
    "devDependencies": {
        "@plurid/plurid-pubsub": "^0.1.7",
        "@types/react": "^16.9.34",
        "@typescript-eslint/eslint-plugin": "^2.29.0",
        "@typescript-eslint/parser": "^2.29.0",
        "eslint": "^6.8.0",
        "rollup": "^2.7.2",
        "rollup-plugin-typescript2": "^0.27.0",
        "typescript": "^3.8.3"
    }
