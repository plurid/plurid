{
    "name": "@plurid/plurid-data",
    "version": "0.1.121",
    "description": "Plurid Constants, Interfaces, Enumerations",
    "keywords": [
        "plurid",
        "data",
        "constants",
        "interfaces",
        "enumerations"
    ],
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/plurid"
    },
    "bugs": {
        "email": "feedback@plurid.com",
        "url": "https://github.com/plurid/plurid/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/plurid/plurid/blob/master/packages/plurid-data/README.md",
    "files": [
        "distribution/"
    ],
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "jsnext:main": "distribution/index.es.js",
    "engines": {
        "node": ">=8",
        "npm": ">=6"
    },
    "scripts": {
        "clean": "rm -rf ./distribution",
        "lint": "eslint ./source --ext .ts,.tsx",
