{
    "extends": [
        "tslint:latest",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/",
            "impl/",
            "coverage/"
        ]
    },
    "rules": {
        "interface-name": false,
        "no-console": false,
        "no-implicit-dependencies": [true, "dev"],
        "ordered-imports": false
    }
}
