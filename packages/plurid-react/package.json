{
    "name": "@plurid/plurid-react",
    "version": "0.1.1",
    "description": "React implementation of Plurid to view and explore the web in three dimensions",
    "keywords": [
        "plurid", "react", "web", "3D", "html"
    ],
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/plurid"
    },
    "bugs": {
        "email": "feedback@plurid.com",
        "url": "https://github.com/plurid/plurid/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/plurid/plurid/blob/master/packages/plurid-react/README.md",
    "files": [
        "dist/"
    ],
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "jsnext:main": "dist/index.es.js",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "clean": "rm -rf ./dist",
        "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
        "test.watch": "react-scripts-ts test --env=jsdom",
        "cypress.open": "cypress open",
        "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
        "build": "yarn clean && yarn test && yarn lint && yarn build.rollup",
        "build.rollup": "rollup -c",
        "start": "rollup -c -w"
    },
    "peerDependencies": {
        "prop-types": ">=15",
        "react": ">=16",
        "react-dom": ">=16",
        "styled-components": ">=4"
    },
    "devDependencies": {
        "@svgr/rollup": "^4.1.0",
        "@types/enzyme": "^3.9.0",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "babel-runtime": "^6.26.0",
        "cross-env": "^5.2.0",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.10.0",
        "prop-types": "^15.7.2",
        "react-scripts-ts": "^3.1.0",
        "react-test-renderer": "^16.8.3",
        "rollup": "^1.4.1",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-commonjs": "^9.2.1",
        "rollup-plugin-node-resolve": "^4.0.1",
        "rollup-plugin-peer-deps-external": "^2.2.0",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-typescript2": "^0.19.3",
        "rollup-plugin-url": "^2.2.0",
        "styled-components": "^4.1.3"
    }
}
