{
    "name": "@plurid/plurid-react",
    "version": "0.1.55",
    "description": "React implementation of Plurid to view and explore the web in three dimensions",
    "keywords": [
        "plurid",
        "react",
        "web",
        "3D",
        "html"
    ],
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/plurid"
    },
    "bugs": {
        "email": "feedback@plurid.com",
        "url": "https://github.com/plurid/plurid/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/plurid/plurid/tree/master/packages/plurid-react",
    "files": [
        "distribution/"
    ],
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "types": "distribution/index.d.ts",
    "engines": {
        "node": ">=8",
        "npm": ">=6"
    },
    "scripts": {
        "clean": "rm -rf ./distribution",
        "spec": "jest",
        "lint": "eslint ./source --ext .ts,.tsx",
        "minify.cjs": "minify ./distribution/index.js --out-file ./distribution/index.min.js",
        "minify": "yarn minify.cjs",
        "start": "yarn clean && rollup -c -w --environment MODE_ENV:local",
        "build.development": "yarn clean && rollup -c --environment MODE_ENV:local",
        "build.production": "yarn clean && rollup -c --environment MODE_ENV:production",
        "build": "yarn clean && yarn lint && yarn spec && yarn build.production && yarn minify",
        "prepublishOnly": "yarn build"
    },
    "peerDependencies": {
        "react": ">=16",
        "react-dom": ">=16",
        "react-redux": ">=7",
        "redux": ">=4",
        "redux-thunk": ">=2",
        "styled-components": ">=4"
    },
    "dependencies": {
        "@plurid/plurid-data": "^0.1.44",
        "@plurid/plurid-engine": "^0.1.19",
        "@plurid/plurid-functions": "^0.1.1",
        "@plurid/plurid-pubsub": "^0.1.3",
        "@plurid/plurid-themes": "^0.1.0",
        "@plurid/plurid-ui-react": "^0.1.31",
        "hammerjs": "^2.0.8",
        "redux-devtools-extension": "^2.13.8"
    },
    "devDependencies": {
        "@babel/core": "^7.7.4",
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/hammerjs": "^2.0.36",
        "@types/jest": "^24.0.23",
        "@types/react": "^16.9.13",
        "@types/react-dom": "^16.9.4",
        "@types/react-redux": "^7.1.5",
        "@types/styled-components": "^4.4.0",
        "@typescript-eslint/eslint-plugin": "^2.9.0",
        "@typescript-eslint/parser": "^2.9.0",
        "babel-minify": "^0.5.1",
        "babel-plugin-styled-components": "^1.10.6",
        "babel-runtime": "^6.26.0",
        "cross-env": "^6.0.3",
        "dotenv-cli": "^3.1.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "eslint": "^6.7.2",
        "jest": "^24.9.0",
        "jest-config": "^24.9.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-redux": "^7.1.3",
        "react-test-renderer": "^16.12.0",
        "redux": "^4.0.4",
        "redux-thunk": "^2.3.0",
        "rollup": "^1.27.8",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.0",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-replace": "^2.2.0",
        "rollup-plugin-typescript2": "^0.25.2",
        "rollup-plugin-url": "^3.0.1",
        "styled-components": "^4.4.1",
        "ts-jest": "^24.2.0",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.2"
    }
