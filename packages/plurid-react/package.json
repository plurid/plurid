{
    "name": "@plurid/plurid-react",
    "version": "0.1.3",
    "description": "React implementation of Plurid to view and explore the web in three dimensions",
    "keywords": [
        "plurid",
        "react",
        "web",
        "3D",
        "html"
    ],
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/plurid"
    },
    "bugs": {
        "email": "feedback@plurid.com",
        "url": "https://github.com/plurid/plurid/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/plurid/plurid/blob/master/packages/plurid-react/README.md",
    "files": [
        "dist/"
    ],
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "jsnext:main": "dist/index.es.js",
    "engines": {
        "node": ">=8",
        "npm": ">=6"
    },
    "scripts": {
        "clean": "rm -rf ./dist",
        "spec": "jest",
        "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
        "build": "yarn clean && yarn build.prod",
        "build.prod": "yarn clean && rollup -c --environment MODE_ENV:production",
        "build.dev": "yarn clean && rollup -c --environment MODE_ENV:local",
        "start": "yarn clean && rollup -c -w --environment MODE_ENV:local",
        "prepublishOnly": "yarn build"
    },
    "peerDependencies": {
        "react": ">=16",
        "react-dom": ">=16",
        "react-redux": ">=7",
        "redux": ">=4",
        "redux-thunk": ">=2",
        "styled-components": ">=4"
    },
    "dependencies": {
        "@plurid/apps.utilities.themes": "^0.1.11",
        "@plurid/plurid-data": "^0.1.1",
        "@plurid/plurid-engine": "^0.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.6.0",
        "@svgr/rollup": "^4.3.2",
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^24.0.18",
        "@types/react": "^16.9.2",
        "@types/react-dom": "^16.9.0",
        "@types/react-redux": "^7.1.2",
        "@types/styled-components": "^4.1.19",
        "babel-plugin-styled-components": "^1.10.6",
        "babel-runtime": "^6.26.0",
        "cross-env": "^5.2.1",
        "dotenv-cli": "^2.0.1",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "jest": "^24.9.0",
        "jest-config": "^24.9.0",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-redux": "^7.1.1",
        "react-test-renderer": "^16.9.0",
        "redux": "^4.0.4",
        "redux-devtools-extension": "^2.13.8",
        "redux-thunk": "^2.3.0",
        "rollup": "^1.21.2",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.0",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-replace": "^2.2.0",
        "rollup-plugin-typescript2": "^0.24.2",
        "rollup-plugin-url": "^2.2.2",
        "styled-components": "^4.1.3",
        "ts-jest": "^24.1.0",
        "ts-node": "^8.3.0",
        "typescript": "^3.6.3"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testEnvironment": "node",
        "testRegex": "(/__specs__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "collectCoverage": true,
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/dist/"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 95,
                "lines": 95,
                "statements": 95
            }
        },
        "collectCoverageFrom": [
            "src/*.{js,ts}"
        ]
    }
}
