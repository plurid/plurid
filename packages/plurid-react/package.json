{
    "name": "@plurid/plurid-react",
    "version": "0.1.94",
    "description": "React implementation of Plurid to explore the web in three dimensions",
    "keywords": [
        "plurid",
        "react",
        "web",
        "3D",
        "html"
    ],
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "repository": {
