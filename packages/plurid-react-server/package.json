{
    "name": "@plurid/plurid-react-server",
    "version": "0.1.8",
    "description": "React implementation of Plurid to view and explore the web in three dimensions with server-side rendering",
    "keywords": [
        "plurid",
        "react",
        "web",
        "3D",
        "html",
        "server-side rendering"
    ],
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/plurid"
    },
    "bugs": {
        "email": "feedback@plurid.com",
        "url": "https://github.com/plurid/plurid/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/plurid/plurid/tree/master/packages/plurid-react-server",
    "files": [
        "distribution/"
    ],
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "types": "distribution/index.d.ts",
    "engines": {
        "node": ">=8",
        "npm": ">=6"
    },
    "scripts": {
        "clean": "rm -rf ./distribution",
        "spec": "jest",
        "lint": "eslint ./source --ext .ts,.tsx",
        "start": "yarn clean && rollup -c rollup.config.development.js -w --environment MODE_ENV:local",
        "build.development": "yarn clean && rollup -c rollup.config.development.js --environment MODE_ENV:development",
        "build.production": "yarn clean && rollup -c --environment MODE_ENV:production",
        "build": "yarn clean && yarn lint && yarn spec && yarn build.production",
        "prepublishOnly": "yarn build"
    },
    "peerDependencies": {
        "open": ">=7",
        "react": ">=16.8",
        "react-dom": ">=16.8",
        "react-helmet-async": ">=1",
        "styled-components": ">=5"
    },
    "dependencies": {
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@rollup/plugin-commonjs": "^11.0.2",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "@rollup/plugin-replace": "^2.3.1",
        "@rollup/plugin-url": "^4.0.2",
        "@types/express": "^4.17.2",
        "@types/jest": "^25.1.3",
        "@types/react": "^16.9.23",
        "@types/react-dom": "^16.9.5",
        "@types/styled-components": "^5.0.1",
        "@typescript-eslint/eslint-plugin": "^2.21.0",
        "@typescript-eslint/parser": "^2.21.0",
        "eslint": "^6.8.0",
        "jest": "^25.1.0",
        "jest-config": "^25.1.0",
        "open": "^7.0.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-helmet-async": "^1.0.4",
        "rollup": "^1.31.1",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-babel-minify": "^9.1.1",
        "rollup-plugin-peer-deps-external": "^2.2.2",
        "rollup-plugin-postcss": "^2.1.1",
        "rollup-plugin-typescript2": "^0.26.0",
        "styled-components": "^5.0.1",
        "ts-jest": "^25.2.1",
        "ts-node": "^8.6.2",
        "typescript": "^3.8.2"
    }
}
