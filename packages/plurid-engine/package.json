        "rollup": "^1.21.4",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-sourcemaps": "^0.4.2",
        "rollup-plugin-typescript2": "^0.24.3",
        "shelljs": "^0.8.3",
        "ts-jest": "^24.1.0",
