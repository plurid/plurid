{
    "name": "@plurid/plurid-engine",
    "version": "0.1.97",
    "description": "Plurid Engine and Utility Functions",
    "keywords": [
        "plurid",
        "engine"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/plurid"
    },
    "bugs": {
        "email": "feedback@plurid.com",
        "url": "https://github.com/plurid/plurid/issues"
    },
    "homepage": "https://github.com/plurid/plurid/tree/master/packages/plurid-engine",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "distribution"
    ],
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "typings": "distribution/index.d.ts",
    "engines": {
        "node": ">=8.0.0"
    },
    "scripts": {
        "clean": "rimraf ./distribution",
        "clean.tests": "find ./distribution -type d -name \"__tests__\" -prune -exec rm -rf {} +",
        "clean.performance": "find ./distribution -type d -name \"__performance__\" -prune -exec rm -rf {} +",
        "test": "jest ./source --config ./jest.config.js --rootDir ./",
        "lint": "eslint ./source --ext .ts,.tsx",
        "start": "yarn clean && rollup -c -w --environment MODE_ENV:local",
        "build.local": "yarn clean && rollup -c --environment ENV_MODE:local",
        "build.development": "yarn clean && rollup -c --environment ENV_MODE:development",
        "build.production": "yarn clean && rollup -c --environment ENV_MODE:production",
        "build": "yarn lint && yarn test && yarn build.production && yarn clean.tests && yarn clean.performance",
        "prepublishOnly": "yarn build"
    },
    "peerDependencies": {
        "@plurid/plurid-data": "*",
        "@plurid/plurid-functions": "*"
    },
    "devDependencies": {
        "@plurid/plurid-data": "^0.1.159",
        "@plurid/plurid-functions": "^0.1.15",
        "@plurid/plurid-pubsub": "^0.1.9",
        "@rollup/plugin-commonjs": "^13.0.0",
        "@types/jest": "^26.0.4",
        "@types/lodash.camelcase": "^4.3.6",
        "@types/node": "^14.0.20",
        "@types/react": "^16.9.41",
        "@types/rollup-plugin-sourcemaps": "^0.4.2",
        "@typescript-eslint/eslint-plugin": "^3.6.0",
        "@typescript-eslint/parser": "^3.6.0",
        "colors": "^1.4.0",
        "coveralls": "^3.1.0",
        "eslint": "^7.4.0",
        "jest": "^26.1.0",
        "jest-config": "^26.1.0",
        "lodash.camelcase": "^4.3.0",
        "prompt": "^1.0.0",
        "react": "^16.13.1",
        "replace-in-file": "^6.1.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.21.0",
        "rollup-plugin-sourcemaps": "^0.6.2",
        "rollup-plugin-typescript2": "^0.27.1",
        "shelljs": "^0.8.4",
        "ts-jest": "^26.1.1",
        "ts-node": "^8.10.2",
        "typedoc": "^0.17.8",
        "typescript": "^3.9.6"
    }
}
