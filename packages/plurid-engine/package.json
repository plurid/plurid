{
    "name": "@plurid/plurid-engine",
    "version": "0.1.16",
    "description": "Plurid Engine and Utility Functions",
    "keywords": [
        "plurid",
        "engine"
    ],
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/plurid"
    },
    "bugs": {
        "email": "feedback@plurid.com",
        "url": "https://github.com/plurid/plurid/issues"
    },
    "homepage": "https://github.com/plurid/plurid/tree/master/packages/plurid-engine",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "distribution"
    ],
    "main": "distribution/plurid-engine.umd.js",
    "module": "distribution/plurid-engine.es5.js",
    "typings": "distribution/index.d.ts",
    "engines": {
        "node": ">=8.0.0"
    },
    "scripts": {
        "clean": "rm -rf ./distribution",
        "clean.specs": "find ./distribution -type d -name \"__specs__\" -prune -exec rm -rf {} +",
        "clean.performance": "find ./distribution -type d -name \"__performance__\" -prune -exec rm -rf {} +",
        "spec": "jest",
        "lint": "eslint ./source --ext .ts",
        "typedoc": "typedoc --out documentation --target es6 --theme minimal --mode file source",
        "start": "yarn clean && rollup -c -w --environment MODE_ENV:local",
        "build.dev": "yarn clean && rollup -c --environment MODE_ENV:local",
        "build.prod": "yarn clean && rollup -c --environment MODE_ENV:production",
        "build": "yarn clean && yarn lint && yarn spec && yarn typedoc && yarn build.prod && yarn clean.specs && yarn clean.performance",
        "prepublishOnly": "yarn build"
    },
    "peerDependencies": {
        "@plurid/plurid-data": "*",
        "@plurid/plurid-functions": "*"
    },
    "devDependencies": {
        "@plurid/plurid-data": "^0.1.35",
        "@plurid/plurid-functions": "^0.1.1",
        "@plurid/plurid-pubsub": "^0.1.1",
        "@types/jest": "^24.0.23",
        "@types/lodash.camelcase": "^4.3.6",
        "@types/node": "^12.12.14",
        "@types/react": "^16.9.13",
        "@types/rollup-plugin-sourcemaps": "^0.4.2",
        "@typescript-eslint/eslint-plugin": "^2.9.0",
        "@typescript-eslint/parser": "^2.9.0",
        "colors": "^1.4.0",
        "coveralls": "^3.0.8",
        "eslint": "^6.7.1",
        "jest": "^24.9.0",
        "jest-config": "^24.9.0",
        "lodash.camelcase": "^4.3.0",
        "prompt": "^1.0.0",
        "react": "^16.12.0",
        "replace-in-file": "^4.2.0",
        "rimraf": "^3.0.0",
        "rollup": "^1.27.5",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-sourcemaps": "^0.4.2",
        "rollup-plugin-typescript2": "^0.25.2",
        "shelljs": "^0.8.3",
        "ts-jest": "^24.2.0",
        "ts-node": "^8.5.2",
        "typedoc": "^0.15.3",
        "typescript": "^3.7.2"
    }
}
