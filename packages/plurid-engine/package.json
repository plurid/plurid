        "access": "public"
    },
    "files": [
        "dist"
    ],
    "main": "dist/plurid-engine.umd.js",
    "module": "dist/plurid-engine.es5.js",
    "typings": "dist/index.d.ts",
    "engines": {
        "node": ">=8.0.0"
    },
    "scripts": {
        "clean": "rm -rf ./dist",
        "spec": "jest",
        "lint": "eslint ./src --ext .ts",
        "typedoc": "typedoc --out docs --target es6 --theme minimal --mode file src",
        "start": "yarn clean && rollup -c -w --environment MODE_ENV:local",
        "build.dev": "yarn clean && rollup -c --environment MODE_ENV:local",
        "build.prod": "yarn clean && rollup -c --environment MODE_ENV:production",
        "build": "yarn clean && yarn lint && yarn spec && yarn typedoc && yarn build.prod",
        "prepublishOnly": "yarn build"
    },
    "peerDependencies": {
        "@plurid/plurid-data": "*",
        "@plurid/utilities.functions": "*"
    },
    "devDependencies": {
