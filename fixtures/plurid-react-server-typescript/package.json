{
    "name": "plurid-react-server-typescript",
    "version": "0.0.0",
    "description": "plurid' web application",
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com)",
    "private": true,
    "main": "build/index.js",
    "scripts": {
        "prestart": "yarn build.production",
        "start": "node build/server.js",
        "start.client.development": "webpack --watch --progress --config scripts/webpack.client.development.js",
        "start.server.development": "yarn build.server && nodemon build/server.js",
        "clean": "rimraf ./build",
        "build.client.development": "webpack --config scripts/webpack.client.development.js",
        "build.client.production": "webpack --config scripts/webpack.client.production.js",
        "build.server": "webpack --config scripts/webpack.server.js",
        "build.development": "yarn clean && yarn build.server && yarn build.client.development",
        "build.production": "yarn clean && yarn build.server && yarn build.client.production"
    },
    "dependencies": {
        "@apollo/react-hooks": "^3.1.3",
        "@plurid/plurid-functions": "^0.1.7",
        "@plurid/plurid-icons-react": "^0.1.18",
        "@plurid/plurid-react": "^0.1.107",
        "@plurid/plurid-react-server": "^0.1.10",
        "@plurid/plurid-themes": "^0.1.26",
        "@plurid/plurid-ui-react": "^0.1.79",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link-http": "^1.5.16",
        "apollo-utilities": "^1.3.3",
        "cross-fetch": "^3.0.4",
        "graphql": "^14.6.0",
        "graphql-tag": "^2.10.3",
        "hammerjs": "^2.0.8",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "react-helmet-async": "^1.0.4",
        "react-redux": "^7.2.0",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "styled-components": "^5.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.8.6",
        "@babel/preset-env": "^7.8.6",
        "@babel/preset-react": "^7.8.3",
        "@types/node": "^13.7.6",
        "@types/react": "^16.9.23",
        "@types/react-dom": "^16.9.5",
        "@types/react-redux": "^7.1.7",
        "@types/styled-components": "^5.0.1",
        "babel-loader": "^8.0.6",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.4.2",
        "file-loader": "^5.1.0",
        "nodemon": "^2.0.2",
        "open": "^7.0.2",
        "react-stripe-elements": "^6.1.0",
        "redux-devtools-extension": "^2.13.8",
        "rimraf": "^3.0.2",
        "source-map-loader": "^0.2.4",
        "style-loader": "^1.1.3",
        "terser-webpack-plugin": "^2.3.5",
        "ts-loader": "^6.2.1",
        "typescript": "^3.8.2",
        "webpack": "^4.41.6",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.11",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
    }
}
